#!../../snabb snsh
local intel = require("apps.intel_mp.intel_mp")
local pci0 = os.getenv("SNABB_PCI_INTEL0")
local pci1 = os.getenv("SNABB_PCI_INTEL1")
local nic = intel.Intel:new({ pciaddr = pci0 })
local tab = nic:rss_tab()
assert(#tab == 0)
assert(tab[0])
local nic0 = intel.Intel:new({pciaddr = pci0, rxq = 0})
local nic1 = intel.Intel:new({pciaddr = pci0, rxq = 1})
tab = nic:rss_tab()
assert(#tab == 1)
assert(tab[0])
assert(tab[1])

local nic2 = intel.Intel:new({pciaddr = pci0, rxq = 2})
local nic3 = intel.Intel:new({pciaddr = pci0, rxq = 3})
tab = nic:rss_tab()
assert(#tab == 3)
assert(tab[2])
assert(tab[3])
main.exit(0)
